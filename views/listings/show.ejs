<% layout("/layouts/template") %>
<body>
     <h1>Listing details</h1>
        <ul>
            <li><%= listing.title %></li>
            <li><%= listing.description %></li>
            <li>$<%= listing.price%></li>
            <li><%= listing.country %></li>
        </ul>
        <a href="/listings/<%=listing.id %>/edit">
